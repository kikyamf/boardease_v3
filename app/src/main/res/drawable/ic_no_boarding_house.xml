        android:fillColor="#CCCCCC"
        android:pathData="M100,30L50,65L100,100L150,65z" />
    
    <!-- Door -->
    <path
        android:fillColor="#BDBDBD"
        android:pathData="M85,120h30v30h-30z" />
    
    <!-- Window -->
    <path
        android:fillColor="#BDBDBD"
        android:pathData="M70,90h20v20h-20z" />
    
    <!-- Plus sign in circle -->
    <path
        android:fillColor="@color/brown"
        android:pathData="M100,140m-15,0a15,15 0,1 1,30 0a15,15 0,1 1,-30 0" />
    
    <!-- Plus horizontal line -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M90,140h20v4h-20z" />
    
    <!-- Plus vertical line -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M100,130v20h4v-20z" />
    
